---
title: Blog Home
load_json: index.json
---
{% extends "_layout.html" %}
{% block content %}
<h4>Latest Posts</h4>
<br />

<div>
    {% for post in posts %}
    <div>
        <a href="posts/{{ post.link }}/">{{ post.title }}</a>
        <p class="lead">{{ post.date }}</p>
    </div>
    <br />
    {% endfor %}
</div>
{% endblock %}
